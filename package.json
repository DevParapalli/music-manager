{
	"name": "music-manager",
	"version": "0.0.1",
	"scripts": {
		"fe:dev": "svelte-kit dev",
		"fe:build": "svelte-kit build",
		"fe:package": "svelte-kit package",
		"fe:preview": "svelte-kit preview",
		"fe:check": "svelte-check --tsconfig ./tsconfig.json",
		"fe:check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
		"fe:lint": "prettier --ignore-path .gitignore --check --plugin-search-dir=. . && eslint --ignore-path .gitignore .",
		"fe:format": "prettier --ignore-path .gitignore --write --plugin-search-dir=. .",
		"be:dev": "python src-python/main.py --dev",
		"be:build": "",
		"be:check": "",
		"be:format": "",
		"dev": "concurrently --kill-others \"npm run be:dev\" \"npm run fe:dev\"",
		"build": "concurrently --kill-others \"npm run be:build\" \"npm run fe:build\""
	},
	"devDependencies": {
		"@sveltejs/adapter-auto": "next",
		"@sveltejs/adapter-static": "^1.0.0-next.21",
		"@sveltejs/kit": "next",
		"@types/cookie": "^0.4.1",
		"@typescript-eslint/eslint-plugin": "^4.31.1",
		"@typescript-eslint/parser": "^4.31.1",
		"concurrently": "^6.4.0",
		"eslint": "^7.32.0",
		"eslint-config-prettier": "^8.3.0",
		"eslint-plugin-svelte3": "^3.2.1",
		"prettier": "^2.4.1",
		"prettier-plugin-svelte": "^2.4.0",
		"svelte": "^3.44.0",
		"svelte-check": "^2.2.6",
		"svelte-preprocess": "^4.9.8",
		"tslib": "^2.3.1",
		"typescript": "^4.4.3",
		"postcss": "^8.3.9",
		"postcss-load-config": "^3.1.0",
		"autoprefixer": "^10.3.7",
		"cssnano": "^5.0.8",
		"tailwindcss": "next",
		"@tailwindcss/forms": "next",
		"@tailwindcss/typography": "next"
	},
	"type": "module"
}
